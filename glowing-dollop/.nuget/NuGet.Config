<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <config>
        <add key="repositoryPath" value="..\packages" />
    </config>

    <packageRestore>
        <add key="enabled" value="True" />
        <add key="automatic" value="True" />
    </packageRestore>

    <activePackageSource>
        <add key="All" value="(Aggregate source)" />
    </activePackageSource>

    <packageSources>
        <add key="Public NuGet Server" value="https://www.nuget.org/api/v2" />
    </packageSources>

    <solution>
        <add key="disableSourceControlIntegration" value="true" />
    </solution>

</configuration>
